    <body class="body-back">
        {# <div class="container col-sm-11 col-xl-9"> #}
        {# <div class="topmenu"> #}

                {% block menu %}
                <h3>Pages:</h3>
                <ul class="nav nav-pills nav-stacked">
                    <li>
                        <a href="{{ path('homepage') }}">Accueil</a>
                    </li>
                </ul>
                {% endblock %}
            </div>
        {# </div> #}

        {% block headerBanner %}
            <div class="banner"> 
                <img alt="bannière principale" class="image_banner" src="/site/assets/images/bg1.jpg"/>
            </div>
        {% endblock %}
        
       
        <div class="container page">

            <div class="page-content row mx-0"> 
